{
  "name": "Infostealer Password Extraction Patterns",
  "description": "Generic password patterns for extracting passwords from Telegram infostealer channel messages",
  "version": "1.0",
  "last_updated": "2025-01-11",
  "patterns": [
    {
      "name": "emoji_pass_code_block",
      "description": "Pattern for [üîë .pass:](link) ```password``` format",
      "pattern": "üîë\\s*\\.?pass:?\\]\\([^)]*\\)\\s*```([^`]+)```",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 1,
      "channels": ["boxed.pw", "redline", "raccoon", "generic"],
      "example": "[üîë .pass:](https://t.me/example) ```mypassword123```"
    },
    {
      "name": "emoji_pass_simple",
      "description": "Pattern for [üîë .pass:](link) password format without code blocks",
      "pattern": "üîë\\s*\\.?pass:?\\]\\([^)]*\\)\\s*([^\\n\\s]+)",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 2,
      "channels": ["boxed.pw", "redline", "raccoon", "generic"],
      "example": "[üîë .pass:](https://t.me/example) mypassword123"
    },
    {
      "name": "password_label_code_block",
      "description": "Pattern for Password: ```password``` format",
      "pattern": "password\\s*:?\\s*```([^`]+)```",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 3,
      "channels": ["generic", "steampay", "mars", "lumma"],
      "example": "Password: ```mypassword123```"
    },
    {
      "name": "pass_label_simple",
      "description": "Pattern for Pass: password format",
      "pattern": "pass\\s*:?\\s*([^\\n\\s]+)",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 4,
      "channels": ["generic", "steampay", "mars", "lumma"],
      "example": "Pass: mypassword123"
    },
    {
      "name": "dot_pass_simple",
      "description": "Pattern for .pass: password format",
      "pattern": "\\.pass\\s*:?\\s*([^\\n\\s]+)",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 5,
      "channels": ["boxed.pw", "redline"],
      "example": ".pass: mypassword123"
    },
    {
      "name": "emoji_direct",
      "description": "Pattern for üîë password (emoji followed by password, not in markdown)",
      "pattern": "üîë\\s+(?![.\\w]*:?\\]\\()([^\\n\\s]+)",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 6,
      "channels": ["boxed.pw", "redline", "raccoon"],
      "example": "üîë mypassword123"
    },
    {
      "name": "standalone_code_block",
      "description": "Pattern for standalone code block containing password",
      "pattern": "```([A-Za-z0-9@#$%^&*!_+-]+)```",
      "flags": ["MULTILINE"],
      "priority": 7,
      "channels": ["generic", "steampay", "mars", "lumma"],
      "example": "```mypassword123```"
    },
    {
      "name": "archive_password",
      "description": "Pattern for archive/zip/rar password indicators",
      "pattern": "(?:archive|zip|rar)\\s*password\\s*:?\\s*([^\\n\\s]+)",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 8,
      "channels": ["generic", "steampay", "mars", "lumma"],
      "example": "Archive password: mypassword123"
    },
    {
      "name": "key_emoji_variants",
      "description": "Pattern for various key emoji variants with password",
      "pattern": "[üîëüóùÔ∏èüîêüîì]\\s*:?\\s*([^\\n\\s]+)",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 9,
      "channels": ["generic", "raccoon", "lumma"],
      "example": "üîê mypassword123"
    },
    {
      "name": "telegram_mention_password",
      "description": "Pattern for @username password format",
      "pattern": "@[A-Za-z0-9_]+\\s*:?\\s*([^\\n\\s]+)",
      "flags": ["MULTILINE"],
      "priority": 10,
      "channels": ["generic", "steampay", "mars"],
      "example": "@LOGACTIVE mypassword123"
    },
    {
      "name": "stealer_specific_formats",
      "description": "Pattern for specific stealer formats like [STEALER] password",
      "pattern": "\\[(?:STEALER|LOGS|ACTIVE|REDLINE|RACCOON|MARS|LUMMA)\\]\\s*:?\\s*([^\\n\\s]+)",
      "flags": ["IGNORECASE", "MULTILINE"],
      "priority": 11,
      "channels": ["redline", "raccoon", "mars", "lumma"],
      "example": "[REDLINE] mypassword123"
    },
    {
      "name": "colon_separated_password",
      "description": "Pattern for simple colon-separated password format",
      "pattern": ":\\s*([A-Za-z0-9@#$%^&*!_+-]{3,50})\\s*$",
      "flags": ["MULTILINE"],
      "priority": 12,
      "channels": ["generic"],
      "example": "Data: mypassword123"
    }
  ],
  "validation_rules": {
    "min_length": 3,
    "max_length": 50,
    "false_positives": [
      "password",
      "pass",
      "123456",
      "admin",
      "user",
      "test",
      "demo",
      "example",
      "sample",
      "default",
      "none",
      "null",
      "empty",
      "qwerty",
      "asdfgh",
      "zxcvbn"
    ],
    "allowed_special_chars": "@#$%^&*!_+-=()[]{}|;:,.<>?/~`",
    "allow_telegram_links": true,
    "allow_at_symbols": true
  },
  "channel_specific_configs": {
    "boxed.pw": {
      "preferred_patterns": ["emoji_pass_code_block", "emoji_pass_simple", "dot_pass_simple"],
      "priority_boost": 2
    },
    "redline": {
      "preferred_patterns": ["stealer_specific_formats", "emoji_direct", "password_label_code_block"],
      "priority_boost": 1
    },
    "raccoon": {
      "preferred_patterns": ["key_emoji_variants", "stealer_specific_formats", "emoji_direct"],
      "priority_boost": 1
    },
    "mars": {
      "preferred_patterns": ["stealer_specific_formats", "telegram_mention_password", "archive_password"],
      "priority_boost": 1
    },
    "lumma": {
      "preferred_patterns": ["stealer_specific_formats", "key_emoji_variants", "password_label_code_block"],
      "priority_boost": 1
    },
    "generic": {
      "preferred_patterns": ["password_label_code_block", "pass_label_simple", "standalone_code_block"],
      "priority_boost": 0
    }
  }
}